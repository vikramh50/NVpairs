{
    "manifest_version": 2,
  
    "name": "NVpairs",
    "description": "Allows you to store name-value pairs in Chrome",
    "version": "1.0",
    "offline_enabled": true,
    "icons": {
      "128": "nv.png"
    },
    "options_ui": {
      "chrome_style": true,
      "page": "nvoptions.html"
    },
    "browser_action": {
      "default_icon": "nv.png",
      "default_popup": "nv.html"
    },
    "permissions" : [
      "storage"
    ]
  }